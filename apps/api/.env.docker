# Environment to use inside the `api` Docker container (service name `mysql` resolves via docker network)
DATABASE_URL="mysql://user:password@mysql:3306/clienthub"

# JWT
JWT_ACCESS_TOKEN_SECRET=replace_access_secret
JWT_REFRESH_TOKEN_SECRET=replace_refresh_secret
ACCESS_TOKEN_EXPIRES_IN=15m
REFRESH_TOKEN_EXPIRES_IN=7d

# App
PORT=4000
FRONTEND_URL=http://localhost:5173
